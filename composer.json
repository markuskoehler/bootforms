{
    "name": "markuskoehler/bootforms",
    "description": "Just a Formbuilder with some Bootstrap specific conveniences. Remembers old input, retrieves error messages and handles all your boilerplate Bootstrap markup automatically.",
    "authors": [
        {
            "name": "Adam Wathan",
            "email": "adam.wathan@gmail.com"
        },
        {
            "name": "Markus Koehler",
            "email": "info@koehler.pro"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "markuskoehler/form": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "AdamWathan\\BootForms": "src/"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/markuskoehler/form.git"
        }
    ]
}
